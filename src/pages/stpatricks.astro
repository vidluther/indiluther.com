---
import BaseLayout from "@layouts/BaseLayout.astro";
import MetaTags from "@components/MetaTags.astro";
import Header from "@components/Header.astro";
---

<BaseLayout>
  <MetaTags
    title="St Patricks 2023 K3/K4 and 4th Grade Play"
    description="Videos from the St. Patricks K3/K4 and 4th grade play ...."
  />

  <div class="flex flex-col items-center justify-center p-4">
    <!-- Video Embed Container -->
    <div class="aspect-w-16 aspect-h-9 max-w-full w-full my-4">
      <!-- Video Iframe -->
      <iframe
        src="https://player.vimeo.com/video/897064226"
        class="w-full h-full"
        frameborder="0"
        allow="autoplay; fullscreen"
        allowfullscreen></iframe>
    </div>

    <!-- Additional videos can be added in the same format -->
  </div>
</BaseLayout>
